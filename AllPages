/* 01 - Remove Ads */
(function adV() {
  if ($('#main-content').html() && document.getElementById('main-content').firstChild) {
    var mc = document.getElementById('main-content');
    if (mc.firstChild.className != '') $(mc.firstChild).remove();
  } else {
    setTimeout(adV, 15);
  }
})();

